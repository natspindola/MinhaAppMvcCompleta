@model DevIO.App.ViewModels.FornecedorViewModel

<div style="padding-top: 20px">
    <div>
        <hr />
        <h4>Endereço</h4>
    </div>

    @if (Model != null)
    {
        <table class="table table-hover">
            <thead class="thead-dark">
                <tr>
                    <th>
                        @Html.DisplayNameFor(expression: model => model.Endereco.Logradouro)
                    </th>
                    <th>
                        @Html.DisplayNameFor(expression: model => model.Endereco.Numero)
                    </th>
                    <th>
                        @Html.DisplayNameFor(expression: model => model.Endereco.Complemento)
                    </th>
                    <th>
                        @Html.DisplayNameFor(expression: model => model.Endereco.Bairro)
                    </th>
                    <th>
                        @Html.DisplayNameFor(expression: model => model.Endereco.Cep)
                    </th>
                    <th>
                        @Html.DisplayNameFor(expression: model => model.Endereco.Cidade)
                    </th>
                    <th>
                        @Html.DisplayNameFor(expression: model => model.Endereco.Estado)
                    </th>
                    <th></th>
                </tr>
            </thead>

            <tr>
                <th>
                    @Html.DisplayFor(expression: model => model.Endereco.Logradouro)
                </th>
                <th>
                    @Html.DisplayFor(expression: model => model.Endereco.Numero)
                </th>
                <th>
                    @Html.DisplayFor(expression: model => model.Endereco.Complemento)
                </th>
                <th>
                    @Html.DisplayFor(expression: model => model.Endereco.Bairro)
                </th>
                <th>
                    @Html.DisplayFor(expression: model => model.Endereco.Cep)
                </th>
                <th>
                    @Html.DisplayFor(expression: model => model.Endereco.Cidade)
                </th>
                <th>
                    @Html.DisplayFor(expression: model => model.Endereco.Estado)
                </th>
                <th>
                    <a class="btn btn-info"><span title="Editar" class="fa fa-pencil-alt"></span></a>
                </th>
            </tr>
        </table>
    }
</div>
